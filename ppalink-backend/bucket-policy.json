{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowPublicReadForAvatarsAndLogos",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": [
        "arn:aws:s3:::ppalink-ted-storage/users/*/avatar/*",
        "arn:aws:s3:::ppalink-ted-storage/agencies/*/logo/*"
      ]
    }
  ]
}
